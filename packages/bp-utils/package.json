{
  "name": "@bp/utils",
  "version": "0.0.0",
  "private": true,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "license": "MIT",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "dev": "tsup src/index.ts --format esm,cjs --watch --dts",
    "lint": "TIMING=1 eslint \"src/**/*.ts*\"",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "test": "jest"
  },
  "devDependencies": {
    "@bp/tsconfig": "*",
    "eslint": "^8.15.0",
    "eslint-config-bp": "*",
    "tsup": "^5.10.1",
    "typescript": "^4.5.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
